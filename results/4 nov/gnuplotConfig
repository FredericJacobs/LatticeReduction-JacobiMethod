set terminal png

# Drawing Hermite over Omega

set output "hermiteOverFactor.png"
set title  "Hermite Factors on random 10-bits matrices"
set xlabel "Dimension"
set ylabel "Hermite Factor"

plot "dataHermiteRandomMatrix_Jacobi.txt" using 1:2 smooth bezier  title "Jacobi(0.6)" with linespoints, \
 "dataHermiteRandomMatrix_LLL.txt" using 1:2 smooth bezier title "LLL(0.99)"  with linespoints

# Drawing orthogonality defect over omega

set output "defect.png"
set title  "Orthogonality Defect on random 10-bits matrices"
set xlabel "Dimension"
set ylabel "Orthogonality Defect"

plot "dataHermiteRandomMatrix_Jacobi.txt" using 1:3 smooth bezier title "Jacobi(0.6)"  with linespoints, \
"dataHermiteRandomMatrix_LLL.txt" using 1:3 smooth bezier title "LLL(0.99)"  with linespoints